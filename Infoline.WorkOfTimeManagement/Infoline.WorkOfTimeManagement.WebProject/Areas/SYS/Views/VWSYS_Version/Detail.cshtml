@model Infoline.WorkOfTimeManagement.BusinessData.VWSYS_Version
@{
    ViewBag.Title = "Kayıt Detay";
    Layout = "~/Views/Shared/_AccountLayout.cshtml";
}

<div class="form-horizontal" data-selector="modalContainer">
    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="versionNumber">versionNumber</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.versionNumber, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"readonly", "readonly"}
            })
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="versionChange">versionChange</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.versionChange, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"readonly", "readonly"}
            })
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="isActive">isActive</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.isActive, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"readonly", "readonly"}
            })
        </div>
    </div>


    <div class="buttons">
        <button class="btn btn-md btn-danger pull-left" data-task="modalClose">Geri</button>
    </div>

</div>
