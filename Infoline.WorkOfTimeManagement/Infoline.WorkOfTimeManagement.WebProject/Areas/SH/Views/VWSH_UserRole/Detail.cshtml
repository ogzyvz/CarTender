@model Infoline.WorkOfTimeManagement.BusinessData.VWSH_UserRole
@{
    ViewBag.Title = "Kayıt Detay";
    Layout = "~/Views/Shared/_AccountLayout.cshtml";
}

<div class="form-horizontal" data-selector="modalContainer">


  

    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="roleid">Yetki</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.Role_Title, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"maxlength","250"},
                {"minlength","2"},
                {"required", "required"},
                {"readonly","readonly"}
            })
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="status">Kullanıcı</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.User_Title, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"placeholder", "Lütfen status Giriniz.."},
                {"maxlength","250"},
                {"minlength","2"},
                {"required", "required"},
                {"readonly","readonly"}
            })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="status_Title">Yetki Durumu</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.status_Title, new Dictionary<string, object>()
            {
                {"class", "form-control"},
                {"maxlength","250"},
                {"minlength","2"},
                {"required", "required"},
                {"readonly","readonly"}
            })
        </div>
    </div>


    <div class="buttons">
        <button class="btn btn-md btn-danger pull-left" data-task="modalClose">Geri</button>
    </div>

</div>

