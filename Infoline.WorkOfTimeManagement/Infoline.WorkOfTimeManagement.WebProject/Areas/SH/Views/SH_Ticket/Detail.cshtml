@model Infoline.WorkOfTimeManagement.BusinessData.VWSH_Ticket
@{
    ViewBag.Title = "Kayıt Detay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-horizontal" data-selector="modalContainer">
 


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="user_Title">Kullanıcı</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.user_Title, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"maxlength","250"},
                {"minlength","2"},
                {"readonly", "readonly"}
            })
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="createtime">Giriş Tarihi</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.createtime, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"maxlength","250"},
                {"minlength","2"},
                {"readonly", "readonly"}
            })
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="endtime">Çıkış Tarihi</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.endtime, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"maxlength","250"},
                {"minlength","2"},
                {"readonly", "readonly"}
            })
        </div>
    </div>


    <div class="form-group">
        <div class="col-md-4">
            <label class="control-label label-md" for="IP">IP</label>
        </div>
        <div class="col-md-8">
            @Html.TextBoxFor(model => model.IP, new Dictionary<string, object>() 
            {
                {"class", "form-control"},
                {"maxlength","250"},
                {"minlength","2"},
                {"readonly", "readonly"}
            })
        </div>
    </div>


    <div class="buttons">
        <button class="btn btn-md btn-danger pull-left" data-task="modalClose">Geri</button>
    </div>

</div>

