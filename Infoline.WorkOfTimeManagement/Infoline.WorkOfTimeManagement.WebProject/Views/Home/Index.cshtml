    <script>
        $(document).ready(function () {
            $(".switcher__button").on('click', function (e) {
                $(".switcher__button").toggleClass('switcher__button--enabled');
                $(".pricing__value").removeClass('pricing__value--hidden');
                $(".pricing__value").toggleClass('pricing__value--show pricing__value--hide');
            });


            var lazy = new LazyLoad();
            lazy.update();

            $("#selectLanguage").find("a").on("click", function (i, e) {
                window.localStorage.language = $(this).text();
                location.reload(true);
            });
            $("#selectLanguage").find("strong").text(window.localStorage.language);

            LanguageTranslate($("*"));

            LanguageTranslate($("*"));

            function LanguageTranslate(source) {

                source
                    .contents()
                    .each(function () {
                        if (this.nodeType === 3) {
                            if (language[this.nodeValue.trim()]) {
                                if (language[this.nodeValue.trim()][window.localStorage.language]) {
                                    this.nodeValue = language[this.nodeValue.trim()][window.localStorage.language];
                                }
                            }
                        }
                    });

                source
                    .each(function () {
                        if (this.hasAttribute("placeholder")) {
                            if (language[this.getAttribute("placeholder").trim()]) {
                                if (language[this.getAttribute("placeholder").trim()][window.localStorage.language]) {
                                    this.setAttribute("placeholder", language[this.getAttribute("placeholder").trim()][window.localStorage.language]);
                                }
                            }
                        }

                        if (this.hasAttribute("title")) {
                            if (language[this.getAttribute("title").trim()]) {
                                if (language[this.getAttribute("title").trim()][window.localStorage.language]) {
                                    this.setAttribute("title", language[this.getAttribute("title").trim()][window.localStorage.language]);
                                }
                            }
                        }

                        var _this = $(this);
                        $.each(_this.data(), function (i, e) {

                            if (i && i == "original-title" && typeof (e) == "string") {
                                if (language[e.trim()]) {
                                    if (language[e][window.localStorage.language]) {
                                        _this.attr("data-" + i, language[e.trim()][window.localStorage.language])
                                    }
                                }
                            }
                        });

                    });

            }
        })

            .on('success', 'form#InsertDemo', function (e, i) {
                if (i.Result) {
                    window.location.replace("http://demo.workoftime.com");
                } else {
                    $('#recap').remove()
                    $('[data-selector="captcha"]').append($('<div id="recap" sitekey="6LfTvMYUAAAAAOPNXLBxr0xO0hTfmv1lPNU9dk4y" class="g-recaptcha"></div>'));
                    grecaptcha.render($('#recap')[0], { sitekey: '6LfTvMYUAAAAAOPNXLBxr0xO0hTfmv1lPNU9dk4y' });
                }
            })


            .on('click', '#contactSend', function (e) {
                e.preventDefault();

                var _this = $(this);
                var action = '/Home/ContactMailSend';
                var method = 'post';
                var data = $('#form').serializeArray();
                if ($.Enumerable.From(data).Where(c => c.value == "").Count() > 0) {
                    MesajWarning("Tüm alanları doldurun veya güvenlik doğrulamasını gerçekleştirin.")
                    return;
                }

                var title = _this.attr("data-loadingtitle") ? _this.attr("data-loadingtitle") : "lütfen bekleyiniz";
                var formData = new FormData();
                var stringData = $('#form').serializeArray();
                var result = null;

                $.each(stringData, function (key, input) {
                    formData.append(input.name, input.value);
                });


                $.ajax({
                    url: action,
                    type: method,
                    data: data,
                    beforeSend: function () {
                        $('body').loadingModal({ text: title, animation: 'rotatingPlane', backgroundColor: 'black' });
                    },
                    success: function (response) {
                        feedback(response.FeedBack);
                        if (response.Result) {
                            var modal = _this.parents(".modal");
                            if (modal.length > 0 && typeof _this.attr('data-modal-close') == "undefined") {
                                modal.modal("hide");
                            }
                            $(".k-pager-refresh.k-link").each(function (i, e) {
                                $(this).trigger("click");
                            });

                            grecaptcha.reset();
                        } else {
                            grecaptcha.reset();
                        }
                        _this.trigger("success", response);
                    },
                    error: function () {
                        feedback("SERVER");
                    },
                    complete: function (response) {
                        $('body').loadingModal('destroy');
                        _this.trigger("complete", response.responseJSON, response.responseText);
                    }
                });


            })

        var cbpAnimatedHeader = (function () {
            var docElem = document.documentElement,
                header = document.querySelector('.navbar-default'),
                didScroll = false,
                changeHeaderOn = 200;
            function init() {
                window.addEventListener('scroll', function (event) {
                    if (!didScroll) {
                        didScroll = true;
                        setTimeout(scrollPage, 250);
                    }
                }, false);
            }
            function scrollPage() {
                var sy = scrollY();
                if (sy >= changeHeaderOn) {
                    $(header).addClass('navbar-scroll')
                }
                else {
                    $(header).removeClass('navbar-scroll')
                }
                didScroll = false;
            }
            function scrollY() {
                return window.pageYOffset || docElem.scrollTop;
            }
            init();

        })();


